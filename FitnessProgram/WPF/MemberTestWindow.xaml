<Window x:Class="FitnessProgram.MemberTestWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Title="MemberTestWindow" Height="450" Width="800">
    <Grid Background="Black">

        <TextBlock x:Name="ActivityBlock" 
                   Foreground="White"
                   FontSize="20"
                   HorizontalAlignment="Left"
                   Margin="277,0,0,0"
                   VerticalAlignment="Center"/>

        <TextBlock x:Name="AmmountBlock"
                   Foreground="White"
                   FontSize="20"
                   HorizontalAlignment="Left"
                   Margin="462,0,0,0"
                   VerticalAlignment="Center"/>

        <Label Content="Aktiviteter"
               Foreground="White"
               HorizontalAlignment="Center"
               Margin="0,35,0,0"
               FontSize="20"
               FontWeight="Bold"
               VerticalAlignment="Top"/>

        <Button Content="Tilbage"
                Background="LightGreen"
                HorizontalAlignment="Right"
                VerticalAlignment="Top"
                Width="60"
                Height="30"
                Margin="0,70,40,0"
                Click="BackButton_nextWindow_Click"/>

    </Grid>
</Window>
